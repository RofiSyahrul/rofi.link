{
  "name": "rofi.link",
  "type": "module",
  "version": "0.0.1",
  "description": "Shorten URLs",
  "author": {
    "name": "Rofi",
    "url": "https://www.rofisyahrul.com"
  },
  "scripts": {
    "astro": "astro",
    "build": "astro check && pnpm sync-and-check && astro build",
    "build:sw": "pnpm sync-and-check && SW_ENABLED=true astro build",
    "build:vercel": "pnpm sync-and-check && DEPLOYMENT_PLATFORM=vercel astro build",
    "dev": "astro sync && DEV_TOOLBAR=true astro dev",
    "dev:no-toolbar": "astro sync && astro dev",
    "prepare": "husky && astro sync && prisma generate",
    "preview": "astro preview",
    "preview:sw": "astro preview --port 9999 --host",
    "prisma:generate": "prisma generate",
    "prisma:migrate-dev": "prisma migrate dev --skip-seed",
    "release": "standard-version",
    "sync-and-check": "astro sync && astro check"
  },
  "dependencies": {
    "@astrojs/check": "^0.7.0",
    "@astrojs/node": "^8.3.2",
    "@astrojs/svelte": "^5.6.0",
    "@astrojs/tailwind": "^5.1.0",
    "@astrojs/vercel": "^7.7.2",
    "@prisma/client": "^5.16.1",
    "@vercel/og": "^0.6.2",
    "astro": "^4.11.5",
    "astrojs-service-worker": "^2.0.0",
    "clsx": "^2.1.1",
    "dotenv": "^16.4.5",
    "typescript": "^5.5.3",
    "ua-parser-js": "^1.0.38"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@eslint/compat": "^1.1.0",
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.6.0",
    "@types/eslint": "^8.56.10",
    "@types/ua-parser-js": "^0.7.39",
    "@typescript-eslint/eslint-plugin": "^7.15.0",
    "@typescript-eslint/parser": "^7.15.0",
    "autoprefixer": "^10.4.19",
    "commitizen": "^4.3.0",
    "cssnano": "^7.0.4",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9.6.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-node": "^0.3.9",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-astro": "^1.2.2",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-import-alias": "^1.2.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-svelte": "^2.41.0",
    "eslint-plugin-unicorn": "^54.0.0",
    "globals": "^15.8.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.7",
    "postcss": "^8.4.39",
    "postcss-cli": "^11.0.0",
    "postcss-html": "^1.7.0",
    "prettier-plugin-astro": "^0.14.0",
    "prettier-plugin-svelte": "^3.2.5",
    "prisma": "^5.16.1",
    "sass": "^1.77.6",
    "standard-version": "^9.5.0",
    "stylelint": "^16.6.1",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-prettier-scss": "^1.0.0",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-recommended": "^14.0.1",
    "stylelint-config-standard-scss": "^13.1.0",
    "stylelint-order": "^6.0.4",
    "svelte": "^4.2.18",
    "svelte-eslint-parser": "^0.39.2",
    "tailwind-scrollbar": "^3.1.0",
    "tailwindcss": "^3.4.4"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx,svelte,astro}": [
      "eslint"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "engines": {
    "node": ">=18"
  }
}
